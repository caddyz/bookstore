/* pages/register/register.wxss */
page{ 
 height: 100%; 
} 

  

.container2 { 
 height: 100%; 
 display: flex; 
 flex-direction: column; 
 padding: 0; 
 box-sizing: border-box; 
 background-color: #f2f2f2
} 

  

/*登录图片*/

.login-icon1{ 
 flex: none; 
} 

.login-img1{ 
 width: 750rpx; 
} 

  

/*表单内容*/
.login-from1 { 
 margin-top: 20px; 
 flex: auto; 
 height:100%; 
} 

  

.inputView1 { 
 background-color: #fff; 
 line-height: 44px; 
} 

/*输入框*/
.nameImage1, .keyImage1 { 
 margin-left: 22px; 
 width: 14px; 
 height: 14px
} 

.loginLab1 { 
 margin: 15px 15px 15px 10px; 
 color: #545454; 
 font-size: 14px

} 

.inputText1 { 
 flex: block; 
 float: right; 
 text-align: right; 
 margin-right: 22px; 
 margin-top: 11px; 
 color: #cccccc; 
 font-size: 14px
} 

  

.line { 
 width: 100%; 
 height: 1px; 
 background-color: #cccccc; 
 margin-top: 1px; 

} 

